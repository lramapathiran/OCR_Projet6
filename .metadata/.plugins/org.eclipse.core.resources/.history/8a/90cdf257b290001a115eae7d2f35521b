package com.lavanya.escalade.model;

import java.util.Date;

public class Topo {
	
	int topoId;
	String topoName;
	String topoLocalization;
	String topoDescription;
	Date topoDate;
	boolean isAvailable;
	
	public Topo() {
		
	}

	public int getTopoId() {
		return topoId;
	}

	public void setTopoId(int topoId) {
		this.topoId = topoId;
	}

	public String getTopoName() {
		return topoName;
	}

	public void setTopoName(String topoName) {
		this.topoName = topoName;
	}

	public String getTopoLocalization() {
		return topoLocalization;
	}

	public void setTopoLocalization(String topoLocalization) {
		this.topoLocalization = topoLocalization;
	}

	public String getTopoDescription() {
		return topoDescription;
	}

	public void setTopoDescription(String topoDescription) {
		this.topoDescription = topoDescription;
	}

	public Date getTopoDate() {
		return topoDate;
	}

	public void setTopoDate(Date topoDate) {
		this.topoDate = topoDate;
	}

	public boolean isAvailable() {
		return isAvailable;
	}

	public void setAvailable(boolean isAvailable) {
		this.isAvailable = isAvailable;
	}
	

}
